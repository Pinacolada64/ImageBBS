{:4022}
	CLOSE 2:GOSUB 1914:IF TR%<1 THEN I=T
{:4024}
	NEXT:RETURN
{:4026}
	CLOSE 2:IF KK=.THEN:&"{f6}{$ce}O {$ce}EW {$ce}EWS.{f6}"
